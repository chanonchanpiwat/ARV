{
  "compilerOptions": {
      "target": "ES2021",
      "module": "CommonJS",
      "outDir": "./dist",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "sourceMap": true,
      "incremental": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
  },
}